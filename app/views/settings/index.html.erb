<h1>Listing settings</h1>

<table>
  <tr>
    <th>Article seo</th>
    <th>Page seo</th>
    <th>Site desc</th>
    <th>Articles display</th>
    <th>Full rss</th>
  </tr>

<% for setting in @settings %>
  <tr>
    <td><%=h setting.article_seo %></td>
    <td><%=h setting.page_seo %></td>
    <td><%=h setting.site_desc %></td>
    <td><%=h setting.articles_display %></td>
    <td><%=h setting.full_rss %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New setting', new_setting_path %>
